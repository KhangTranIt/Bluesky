<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>New Town</title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />
        <link href="@Url.Content("~/Content/kendo/kendo.common.min.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/kendo/kendo.metro.min.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/bootstrap/bootstrap.min.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/bootstrap/metisMenu.min.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/bootstrap/sb-admin-2.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/bootstrap/font-awesome.min.css")" rel="stylesheet" type="text/css">
        <link href="@Url.Content("~/Content/themes/base/jquery-ui.css")" rel="stylesheet"  type="text/css" />
        <link href="@Url.Content("~/Content/style.css")" rel="stylesheet"  type="text/css" />
        <script src="@Url.Content("~/Scripts/jquery-1.10.2.min.js")" type="text/javascript"></script>
        @RenderSection("leftmainmenuhead", required: false)
    </head>
    <body>
        @Html.Action("AdminLink", "Common")
    
        <div id="wrapper">
            <!-- Navigation -->
            @if (IsSectionDefined("leftmainmenu"))
            {
                @RenderSection("leftmainmenu")
            }
            <div id="page-wrapper">
                @RenderBody()
            </div>
            <footer>
                <div class="col-sm-12">
                    <div class="text-right">
                        <ul class="list-inline">
                            <li>NewTown33.com<span id="copyright-year"></span> &copy; <a href="index-5.html">Privacy Policy</a></li> |
                            <li><a href="#">About Us</a></li> |
                            <li><a href="#">Responsible Gaming</a></li> |
                            <li><a href="#">Terms and Conditions</a></li> |
                            <li><a href="#">Contact Us</a></li>
                        </ul>
                    </div>
                </div>
            </footer>
        </div>

        <!-- Bootstrap Core JavaScript -->
        <script src="@Url.Content("~/Scripts/bootstrap.min.js")" type="text/javascript"></script>

        <!-- Metis Menu Plugin JavaScript -->
        <script src="@Url.Content("~/Scripts/metisMenu.min.js")" type="text/javascript"></script>

        <!-- Custom Theme JavaScript -->
        <script src="@Url.Content("~/Scripts/sb-admin-2.js")" type="text/javascript"></script>

        <!-- Kendo ui -->
        <script src="@Url.Content("~/Scripts/kendo.all.min.js")" type="text/javascript"></script>
    </body>
</html>
